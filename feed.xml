---
layout: nil
permalink: /feed/index.html
---
<?xml version="1.0" encoding="utf-8" ?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Douglas Hunley</title>
    <link href="http://hunleyd.github.io" />
    <link href="http://hunleyd.github.io/feed/" rel="self" />
    <id>http://hunleyd.github.io</id>
    <updated>{{site.posts[0].date | date_to_xmlschema}}</updated>
    <author><name>Douglas Hunley</name></author>
    {% for post in site.posts %}
    <entry>
        <title>{{post.title}}</title>
        <link href="{{post.url}}" />
        <id>{{post.url}}</id>
        <published>{{post.date | date_to_xmlschema}}</published>
        <updated>{{post.date | date_to_xmlschema}}</updated>
        <content type="xhtml">
            <div xmlns="http://www.w3.org/1999/xhtml">
                {{post.content}}
            </div>
        </content>
    </entry>
    {% endfor %}
</feed>
